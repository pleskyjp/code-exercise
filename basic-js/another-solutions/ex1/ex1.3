// 1.3 find modus
let input4 = [1, 2, 2, 4, 6, 9, 11, 12, 11, 666];

// hodnota zastoupena nejcasteji
function findModus(arr) {
  let counts = {};
  let largest = 0;

  for (let element of arr) {
    // TOTO CELE MUZEME ZAPSAT POMOCI TERNARNIHO OPERATORU
    // if (counts[element] === undefined) { // !counts[element]
    //   counts[element] = 0
    // }
    // counts[element] += 1;

    // ternarni operator
    counts[element] = counts[element] ? counts[element] += 1 : 1;

    if (counts[element] > largest) largest = counts[element];
  }

  let result = [];
  for (let key in counts) {
    if (counts[key] === largest) result.push(parseInt(key)) // Number()
  }

  return result
}

console.log(findModus(input4)); //[ 2, 11 ]